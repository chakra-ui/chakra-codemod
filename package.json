    "is-git-clean": "1.1.0",
    "jscodeshift": "^0.11.0",
    "meow": "7.0.1"
  },
  "devDependencies": {
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.15",
    "@types/jscodeshift": "^0.7.1",
    "@types/node": "^14.14.7",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5"
  },
  "files": [
    "dist/transforms/*.js",
    "dist/bin/*.js"
  ],
  "scripts": {
